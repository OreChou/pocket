<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.orechou.pocket">

    <application
        android:name=".PocketApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.habit.HabitActivity"></activity>
        <activity android:name=".views.code.CodeActivity" />
        <activity android:name=".views.clipboard.ClipboardActivity" />
        <activity android:name=".views.deadline.DeadlineActivity" />
        <activity
            android:name=".views.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>